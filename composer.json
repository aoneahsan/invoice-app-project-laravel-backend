{
    "name": "zaions/invoice-generator-app",
    "type": "project",
    "description": "The Best Invoice Generator SaaS App available online, create your account today and get started.",
    "keywords": [
        "zaions",
        "ahsan mahmood",
        "invoice app",
        "invoice generator app",
        "saas app",
        "best free online app"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel/fortify": "^1.20.1",
        "laravel/framework": "^10.44",
        "laravel/sanctum": "^3.3.3",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.27",
        "spatie/laravel-permission": "^6.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.13.11",
        "laravel/sail": "^1.27.4",
        "mockery/mockery": "^1.6.7",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.5.10",
        "spatie/laravel-ignition": "^2.4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan cache:clear",
            "@php artisan optimize:clear",
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan cache:clear",
            "@php artisan optimize:clear",
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
